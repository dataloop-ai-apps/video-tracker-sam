{
  "name": "video-tracker-sam",
  "displayName": "Video Tracker Sam",
  "description": "Online Video Tracker based on SAM",
  "version": "1.0.13",
  "attributes": {
    "Provider": "Other",
    "Category": "Application",
    "Application Type": ["Model", "Function"],
    "License": "Apache 2.0"
  },
  "scope": "public",
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/video-tracker-sam.git",
    "gitTag": "1.0.13"
  },
  "components": {
    "toolbars": [
      {
        "displayName": "SAM Tracker",
        "invoke": {
          "type": "function",
          "namespace": "video-tracker-app.video-tracker-app.run"
        },
        "location": "videoTracker",
        "icon": "icon-dl-edit"
      }
    ],
    "modules": [
      {
        "name": "video-tracker-app",
        "entryPoint": "tracker_main.py",
        "className": "ServiceRunner",
        "initInputs": [],
        "functions": [
          {
            "name": "run",
            "description": "Sam Video Tracker",
            "input": [
              {
                "name": "item_stream_url",
                "type": "Json"
              },
              {
                "name": "bbs",
                "type": "Json"
              },
              {
                "name": "start_frame",
                "type": "Json"
              },
              {
                "name": "frame_duration",
                "type": "Json"
              }
            ],
            "output": [
              {
                "name": "output_dict",
                "type": "Json"
              }
            ]
          }
        ]
      }
    ],
    "services": [
      {
        "name": "video-tracker-app",
        "moduleName": "video-tracker-app",
        "runtime": {
          "podType": "gpu-t4-m",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/gpu/video-tracker-sam:0.1.10",
          "autoscaler": {
            "minReplicas": 0,
            "maxReplicas": 2,
            "queueLength": 100
          }
        },
        "executionTimeout": 36000
      }
    ]
  }
}